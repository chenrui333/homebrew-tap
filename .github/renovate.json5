{
  extends: [
    'config:recommended',
    'docker:pinDigests',
    'helpers:pinGitHubActionDigests',
    ':configMigration',
    ':pinDevDependencies',
  ],
  commitMessageSuffix: ' in {{packageFile}}',
  automerge: true,
  automergeType: 'pr',
  automergeStrategy: 'squash',
  baseBranchPatterns: [
    'main',
  ],
  platformAutomerge: false,
  labels: [
    'CI-syntax-only',
    'dependencies',
    'renovate',
  ],
  vulnerabilityAlerts: {
    enabled: true,
    labels: [
      'security',
    ],
  },
}
